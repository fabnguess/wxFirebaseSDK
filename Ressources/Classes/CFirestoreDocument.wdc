   -
     name : ObtenirValeurEnum
     procedure_id : 1682208157213925769
     type_code : 12
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] ObtenirValeurEnum (<nTypeDefinition> est entier)
      //
      // Paramètres :
      //	nTypeDefinition (entier) : <indiquez ici le rôle de nTypeDefinition>
      // Valeur de retour :
      // 	chaîne ANSI : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      procédure global ObtenirValeurEnum(local nTypeDefinition est un entier):chaîne
      
      sValeurEnum est une chaîne
      
      SELON nTypeDefinition
      	CAS wlBooléen
      		sValeurEnum = EnumBoolean..Valeur
      	CAS wlChaîne, wlChaîneAnsi, wlChaîneUnicode
      		sValeurEnum = EnumChaine..Valeur
      	CAS wlEntier
      		sValeurEnum = EnumEntier..Valeur
      	CAS wlRéel
      		sValeurEnum = EnumReel..Valeur
      	CAS wlTableau
      		sValeurEnum = EnumTableau..Valeur
      		
      FIN	
      
      RENVOYER sValeurEnum
