       {"m_jsonDocumentTemp.[%CONST_DONNEES_UTILISATEUR%].[%sNomDuchamp%].[%EnumTableau..Valeur%].values[[%nClé%]].[%ObtenirValeurEnum(TypeVar(Valeur))%]", indVariable} =  Valeur
      
